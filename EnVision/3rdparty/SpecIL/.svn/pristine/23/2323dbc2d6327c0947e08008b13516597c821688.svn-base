<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<ImportGroup Label="PropertySheets" />
	<PropertyGroup Label="UserMacros" />
	<PropertyGroup />

	<!-- PropertySheet fuer die Verwendung von big libtiff
	-->

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
<!-- 		<LIBTIFF>C:\Libraries\tiff-4.0.6</LIBTIFF>-->
		<LIBTIFF>$(IFFLIBRARYPATH)\libtiff\4.0.6</LIBTIFF>
		<PATH>$(LIBTIFF)\libtiff%3b$(PATH)</PATH>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|Win32'">
<!-- 		<LIBTIFF>C:\Libraries\tiff-4.0.6</LIBTIFF>-->
		<LIBTIFF>$(IFFLIBRARYPATH)\libtiff\4.0.6</LIBTIFF>
		<PATH>$(LIBTIFF)\libtiff%3b$(PATH)</PATH>
	</PropertyGroup>
	
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
<!-- 		<LIBTIFF>C:\Libraries\tiff-4.0.6</LIBTIFF>-->
		<LIBTIFF>$(IFFLIBRARYPATH)\libtiff\4.0.6</LIBTIFF>
		<PATH>$(LIBTIFF)\libtiff%3b$(PATH)</PATH>
	</PropertyGroup>
	
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
<!-- 		<LIBTIFF>C:\Libraries\tiff-4.0.6</LIBTIFF>-->
		<LIBTIFF>$(IFFLIBRARYPATH)\libtiff\4.0.6</LIBTIFF>
		<PATH>$(LIBTIFF)\libtiff%3b$(PATH)</PATH>
	</PropertyGroup>
	
	<ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'">
		<ClCompile>
			<AdditionalIncludeDirectories>$(LIBTIFF)\libtiff\;%(AdditionalIncludeDirectories)
			</AdditionalIncludeDirectories>
		</ClCompile>
		<Link>
			<AdditionalLibraryDirectories>$(LIBTIFF)\libtiff\;%(AdditionalLibraryDirectories)
			</AdditionalLibraryDirectories>
			<AdditionalDependencies>libtiff_i.lib;%(AdditionalDependencies)
			</AdditionalDependencies>
		</Link>
	</ItemDefinitionGroup>
	
	<ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Release|Win32'">
		<ClCompile>
			<AdditionalIncludeDirectories>$(LIBTIFF)\libtiff\;%(AdditionalIncludeDirectories)
			</AdditionalIncludeDirectories>
		</ClCompile>
		<Link>
			<AdditionalLibraryDirectories>$(LIBTIFF)\libtiff\;%(AdditionalLibraryDirectories)
			</AdditionalLibraryDirectories>
			<AdditionalDependencies>libtiff_i.lib;%(AdditionalDependencies)
			</AdditionalDependencies>
		</Link>
	</ItemDefinitionGroup>
	
	<ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
		<ClCompile>
			<AdditionalIncludeDirectories>$(LIBTIFF)\libtiff\;%(AdditionalIncludeDirectories)
			</AdditionalIncludeDirectories>
		</ClCompile>
		<Link>
			<AdditionalLibraryDirectories>$(LIBTIFF)\libtiff\;%(AdditionalLibraryDirectories)
			</AdditionalLibraryDirectories>
			<AdditionalDependencies>libtiff_i.lib;%(AdditionalDependencies)
			</AdditionalDependencies>
		</Link>
	</ItemDefinitionGroup>
	
	<ItemDefinitionGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
		<ClCompile>
			<AdditionalIncludeDirectories>$(LIBTIFF)\libtiff\;%(AdditionalIncludeDirectories)
			</AdditionalIncludeDirectories>
		</ClCompile>
		<Link>
			<AdditionalLibraryDirectories>$(LIBTIFF)\libtiff\;%(AdditionalLibraryDirectories)
			</AdditionalLibraryDirectories>
			<AdditionalDependencies>libtiff_i.lib;%(AdditionalDependencies)
			</AdditionalDependencies>
		</Link>
	</ItemDefinitionGroup>
	
</Project>